{
  "name": "@floorplan/react",
  "version": "0.0.5",
  "description": "The place to start for making a component library.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "start": "yarn storybook",
    "dev": "yarn storybook",
    "precommit": "lint-staged",
    "build-storybook": "build-storybook",
    "add-component": "node ./scripts/addComponent.js addComponent",
    "link": "./scripts/link.sh",
    "build:link": "rm -rf ./dist/* && node ./scripts/build-dist.js",
    "watch": "yarn run link && yarn build:rollup -w",
    "build": "NODE_ENV=development rollup --config rollup.config.js",
    "build:prod": "yarn build:rollup",
    "publish:npm": "./scripts/publish-dist.sh",
    "build:rollup": "NODE_ENV=production rollup --silent --config rollup.config.js && NODE_ENV=development rollup --silent --config rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floorplan/floorplan-base.git"
  },
  "keywords": [
    "component",
    "library"
  ],
  "author": "Brian Bartholomew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floorplan/floorplan-base/issues"
  },
  "homepage": "https://github.com/floorplan/floorplan-base#readme",
  "devDependencies": {
